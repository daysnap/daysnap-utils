{
  "name": "@daysnap/utils",
  "version": "0.0.35",
  "description": "通用的工具库",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "gen": "plop --plopfile ./scripts/gen.js --force",
    "del": "plop --plopfile ./scripts/del.js --force",
    "entry": "plop --plopfile ./scripts/entry.js --force",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "build": "dsc build",
    "test": "jest",
    "coverage": "jest --coverage",
    "prerelease": "npm run entry && npm run test && npm run docs",
    "release": "npm run build && dsc publish -t",
    "husky": "husky install && echo 'export PATH=\"/usr/local/bin/:$PATH\"' >> ~/.huskyrc",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "prepare": "husky install && echo 'export PATH=\"/usr/local/bin/:$PATH\"' >> ~/.huskyrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daysnap/utils.git"
  },
  "keywords": [
    "daysnap",
    "utils",
    "tool"
  ],
  "files": [
    "lib",
    "es",
    "docs"
  ],
  "author": "woshiajuana <979703986@qq.com>",
  "sideEffects": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daysnap/utils/issues"
  },
  "homepage": "https://github.com/daysnap/utils#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "node-dir": "^0.1.17",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4"
  }
}
