{
  "name": "@daysnap/utils",
  "version": "0.0.3",
  "description": "通用的工具库",
  "scripts": {
    "ts-node": "npx ts-node --project ./tsconfig.json ./src/filename.ts",
    "build": "dsc build",
    "test": "jest",
    "coverage": "jest --coverage",
    "publish": "npm run build && dsc publish",
    "husky": "husky install && echo 'export PATH=\"/usr/local/bin/:$PATH\"' >> ~/.huskyrc",
    "lint": "eslint 'src/**/*.{js,ts}' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daysnap/utils.git"
  },
  "keywords": [
    "daysnap",
    "utils",
    "tool"
  ],
  "files": [
    "lib",
    "es"
  ],
  "author": "woshiajuana <979703986@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daysnap/util/issues"
  },
  "homepage": "https://github.com/daysnap/util#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "commitizen": "^4.2.4",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHooks": {
